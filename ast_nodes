
Node
--

Function > Node
--
-name:Symbol
-statements:List<Node>

Symbol > Node
--
-id:String

Expression > Node
--
-nodes:Node

Variable > Node
--
-name:Symbol
-value:Expression

Constant > Node
--
-name:Symbol
-value:Expression

Assignment > Node
--
-leftHandSide:Symbol
-rightHandSide:Expression

Loop > Node
--
-condition:Expression
-statements:List<Node>

Branch > Node
--
-condition:Expression
-ifStatements:List<Node>
-elseStatements:List<Node>

RelationOperator > Node
--
-operator:Token
-leftHandSide:Expression
-rightHandSide:Expression

BinaryOperator > Node
--
-operator:Token
-leftHandSide:Expression
-rightHandSide:Expression

UnaryOperator > Node
--
-operator:Token
-operand:Expression

Value > Node
--
-value:Token

Call > Node
--
-name:Symbol
-arguments:List<Expression>
