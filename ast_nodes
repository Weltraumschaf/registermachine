
AstNode
--

Function > AstNode
--
-name:Symbol
-statements:List<AstNode>

Symbol > AstNode
--
-id:String

Expression > AstNode
--
-nodes:AstNode

Variable > AstNode
--
-name:Symbol
-value:Expression

Constant > AstNode
--
-name:Symbol
-value:Expression

Assignment > AstNode
--
-leftHandSide:Symbol
-rightHandSide:Expression

Loop > AstNode
--
-condition:Expression
-statements:List<AstNode>

Branch > AstNode
--
-condition:Expression
-ifStatements:List<AstNode>
-elseStatements:List<AstNode>

RelationOperator > AstNode
--
-operator:Token
-leftHandSide:Expression
-rightHandSide:Expression

BinaryOperator > AstNode
--
-operator:Token
-leftHandSide:Expression
-rightHandSide:Expression

UnaryOperator > AstNode
--
-operator:Token
-operand:Expression

Value > AstNode
--
-value:Token

Call > AstNode
--
-name:Symbol
-arguments:List<Argument>

Argument > AstNode
--
-name:Symbol
-arguments:List<Expression>

